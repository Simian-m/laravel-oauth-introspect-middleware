{
  "name": "simianbv/introspect",
  "description": "Laravel Middleware for letting a resource owner verify a OAuth2 access tokens with a remote authorization server",
  "type": "project",
  "keywords": [
    "laravel",
    "introspect"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Admin",
      "email": "it@simian.nl"
    },
    {
      "name": "Arie Timmerman",
      "email": "arietimmerman@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "guzzlehttp/guzzle": "~6.0",
    "laravel/framework": "6.*|7.*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Simianbv\\Introspect\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Simianbv\\Introspect\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "beta"
}
